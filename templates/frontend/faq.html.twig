{% extends 'frontend/base.html.twig' %}

{% block title %}Reise FAQ für kuba{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('styles/faq.css') }}" type="text/css">
{% endblock %}

{% block body %}
    <main class="main">

        {{ include ('frontend/components/global/_hero.html.twig') }}

        {% include('frontend/components/global/_breadcrumb.html.twig')
            with {'pages': ['Start', 'Reise FAQ für kuba']} %}

        {{ include ('frontend/components/faq/_intro.html.twig') }}

        {{ include ('frontend/components/faq/_content_index.html.twig') }}

        {{ include ('frontend/components/faq/_question.html.twig') }}

    </main>
{% endblock %}


{% block javascripts %}
    <script src="{{ asset('scripts/faq.js') }}"></script>
{% endblock %}